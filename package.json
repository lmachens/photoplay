{
  "name": "vite-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "client:dev": "vite",
    "server:dev": "ts-node --project tsconfig.server.json src/server",
    "server:build": "tsc --project tsconfig.server.json",
    "storybook:build": "build-storybook -o dist/storybook",
    "client:build": "vite build",
    "build": "npm run server:build && npm run client:build && npm run storybook:build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\" && npm run jest",
    "storybook": "start-storybook -s src/app/assets -p 6006",
    "start": "node dist/server.js",
    "jest": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@storybook/addon-actions": "^6.3.0-rc.10",
    "@storybook/addon-essentials": "^6.3.0-rc.10",
    "@storybook/addon-links": "^6.3.0-rc.10",
    "@storybook/react": "^6.3.0-rc.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/mongodb": "^3.6.17",
    "@types/node": "^15.0.2",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "http-proxy-middleware": "^2.0.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "storybook-builder-vite": "^0.0.9",
    "storybook-css-modules-preset": "^1.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "ts-node": "^9.1.1",
    "typescript": "4.2.4",
    "vite": "^2.2.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.9",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
